{
  "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "rootDir": "./",
      "moduleResolution": "node",
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}